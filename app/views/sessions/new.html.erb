<h1>Sign in</h1>
<p><%= link_to 'Sign up', signup_path %></p>
<%= form_for :user, url: {action:"create"} do |f| %>
  <p>
    <%= f.label :email %><br>
    <%= f.text_field :email %>
  </p>
 
  <p>
    <%= f.label :password %><br>
    <%= f.password_field :password_hash %>
  </p>
 
  <p>
    <%= f.submit "Sign in" %>
  </p>
<% end %>
